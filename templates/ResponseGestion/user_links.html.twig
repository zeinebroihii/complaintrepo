<script>
    // Function to filter list group items by name
    function filterByName() {
        var input, filter, listGroup, items, item, username, i;
        input = document.getElementById('name-filter');
        filter = input.value.toUpperCase();
        listGroup = document.querySelector('.list-group');
        items = listGroup.querySelectorAll('.user-link');
        
        for (i = 0; i < items.length; i++) {
            item = items[i];
            username = item.textContent || item.innerText;
            if (username.toUpperCase().indexOf(filter) > -1) {
                item.style.display = '';
            } else {
                item.style.display = 'none';
            }
        }
    }

    // Add event listener to the name filter input
    document.getElementById('name-filter').addEventListener('input', filterByName);
</script>


