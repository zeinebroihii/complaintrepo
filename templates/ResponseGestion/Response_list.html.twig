<section class="inner-page">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-lg-8">
                <div class="text-center mb-4">
                    <h1>List of Responses</h1>
                    <p>Complaint ID: {{ complaint.id }}</p>
                </div>
                <table class="table">
                    <thead>
                        <tr>
                            <th>Date</th>
                            <th>Response</th>
                            <th>Actions</th>
                        </tr>
                    </thead>
                    <tbody>
                        {% for response in responses %}
                        <tr>
                            <td>{{ response.date|date('Y-m-d H:i:s') }}</td>
                            <td>{{ response.response }}</td>
                            <td>
                                {% if not response.seenByUser %}
                                    <a href="{{ path('edit_response', {'responseId': response.id}) }}" class="btn btn-primary"><i class="bi bi-pencil-fill"></i></a>
                                    <a href="{{ path('delete_response', {'responseId': response.id}) }}" class="btn btn-danger"><i class="bi bi-trash-fill"></i></a>
                                {% endif %}
                            </td>
                        </tr>
                        {% endfor %}
                    </tbody>
                </table>
                <div class="text-center mt-4">
<a href="{{ path('complaint_response_list', {'complaintId': complaint.id}) }}" class="btn btn-primary">
    <i class="bi bi-arrow-left"></i> Back to Response List
</a>
                </div>
            </div>
        </div>
    </div>
</section>


